{
  "name": "A-ERA-sorteos",
  "version": "1.0.0",
  "description": "Sistema de sorteos para Instagram",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node --watch server/index.js",
    "client": "cd client && npm run dev",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build"
  },
  "keywords": [
    "instagram",
    "sorteos",
    "rifas",
    "giveaway"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "cheerio": "^1.0.0-rc.12",
    "better-sqlite3": "^9.2.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
